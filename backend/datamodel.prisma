type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Meeting {
  id: ID! @unique
  title: String!
  description: String!
  image: String
  largeImage: String
  startTime: Int!
  endTime: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  # user: User!
}

type Item {
  id: ID! @unique
  title: String!
}
